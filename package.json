{
  "name": "techfixtracker",
  "version": "1.0.0",
  "image_name_and_version": "11111111111.dkr.ecr.us-east-1.amazonaws.com/techfixtracker:1.0.0",
  "image_name": "11111111111.dkr.ecr.us-east-1.amazonaws.com/techfixtracker",
  "aws_ecr": "11111111111.dkr.ecr.us-east-1.amazonaws.com",
  "aws_ecr_region": "us-east-1",
  "description": "",
  "license": "ISC",
  "repository": {
    "url": "https://github.com/JJimenez18/microservices_backend_techfix_tracker.git"
  },
  "scripts": {
    "eslint": "eslint . --ext .js,.ts",
    "eslint:fix": "npm run eslint -- --fix",
    "test": "jest --runInBand --detectOpenHandles",
    "dev": "ts-node-dev -r dotenv/config src/index.ts",
    "clean": "rimraf build",
    "build": "tsc",
    "reinstall": "npm un @chtalent/apis-common && npm i @chtalent/apis-common@1.0.19 --save-exact",
    "re:common": "npm un @chtalent/tools-models-common && npm i @chtalent/tools-models-common",
    "clean:build": "npm run clean && tsc",
    "docker:rmi:none": "docker rmi $(docker images --filter \"dangling=true\" -q --no-trunc)",
    "docker:rmi": "docker rmi $(docker images | grep $npm_package_image_name) || true",
    "docker:image": "docker build --network=host -t $npm_package_image_name_and_version . && docker image prune --filter label=stage=builder_$npm_package_name -f",
    "docker:build": "npm run docker:rmi && npm run docker:image",
    "docker:ecr:login": "aws ecr get-login-password --region $npm_package_aws_ecr_region | docker login --username AWS --password-stdin $npm_package_aws_ecr",
    "docker:ecr:push": "npm run docker:ecr:login && docker push $npm_package_image_name_and_version"
  },
  "dependencies": {
    "@chtalent/apis-common": "1.0.19",
    "@chtalent/tools-models-common": "^1.0.34",
    "@types/mongoose-aggregate-paginate-v2": "1.0.1",
    "aws-sdk": "^2.1354.0",
    "axios": "^0.21.1",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "exceljs": "^4.3.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-validator": "^6.11.1",
    "fs-extra": "^10.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "5.13.15",
    "mysql2": "^2.2.5",
    "node-telegram-bot-api": "^0.66.0",
    "nodemailer": "^6.7.2",
    "qs": "^6.11.2",
    "soap": "^0.36.0",
    "uuid": "^9.0.1",
    "xml-js": "1.6.11",
    "xml2js": "0.6.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.7",
    "@types/fs-extra": "^9.0.13",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node-telegram-bot-api": "^0.64.6",
    "@types/nodemailer": "^6.4.4",
    "@types/uuid": "^9.0.7",
    "@types/xml2js": "0.4.11",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.6.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "jest": "^26.6.3",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4",
    "ts-node-dev": "^1.0.0-pre.56",
    "typescript": "5.0.2"
  }
}
